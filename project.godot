config_version=5

[application]

config/name="Solace"
config/description="An umbrella project for tooling, references and prototypes under the same framework.
"
config/version="0.0.0a"
run/main_scene="res://main/Main.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://main/icon.svg"

[autoload]

SolaceRuntimeAutoload="*res://addons/solace_core_plugin/core/SolaceRuntimeAutoload.tscn"

[display]

