window/size/viewport_height=1080
window/size/mode=2
window/size/initial_position_type=2
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Solace"

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/gdUnit4/plugin.cfg", "res://addons/solace_core_plugin/plugin.cfg")

